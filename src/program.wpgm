import wollok.game.*
import objetos.*

program futbolito{

	game.addVisualCharacter(lionel)
	game.addVisual(pelota)
	game.cellSize(40)
	game.width(20)
	game.height(10)
	keyboard.enter().onPressDo({game.say(lionel, "Vamos Argentina")})
	keyboard.t().onPressDo({lionel.taquito()})
	keyboard.c().onPressDo({lionel.cambiarCamiseta()})
	game.start()


}